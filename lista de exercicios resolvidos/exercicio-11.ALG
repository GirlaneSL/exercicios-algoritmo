algoritmo "Exercício-11"
var
   a, b, c: inteiro
   delta: real
inicio
      Escreva("Digite um valor A: ")
      Leia(a)
      Escreva("Digite um valor B: ")
      Leia(b)
      Escreva("Digite um valor C: ")
      Leia(c)
      
      delta <- b^2 - 4 * a * c
      
      Escreva("O valor de delta é", delta)
fimalgoritmo